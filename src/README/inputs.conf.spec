[ftp://default]
* Configure an input for retrieving information via FTP

port = <value>
* The port to run the FTP server on

path = <value>
* The path to place the files into

address = <value>
* The address to have the FTP server listen on

certfile = <value>
* The path to the certificate file

keyfile = <value>
* The path to the key file
